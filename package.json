{
  "name": "minimized-reown-appkit",
  "version": "1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@reduxjs/toolkit": "^1.9.0",
    "@reown/appkit": "1.1.0",
    "@reown/appkit-adapter-wagmi": "1.1.0",
    "@reown/appkit-siwe": "1.1.0",
    "@tanstack/react-query": "5.59.8",
    "@vitest/coverage-c8": "^0.33.0",
    "axios": "^1.1.3",
    "env-cmd": "^10.1.0",
    "loglevel": "^1.8.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "4.0.13",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "uuid": "^9.0.0",
    "viem": "2.21.21",
    "wagmi": "2.12.17"
  },
  "devDependencies": {
    "@redux-devtools/core": "^3.13.1",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.3",
    "cpy-cli": "^4.2.0",
    "cross-var": "^1.1.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.79.5",
    "typescript": "5.5.4",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-svgo": "^1.6.2",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.3"
  },
  "scripts": {
    "preview": "vite preview",
    "build": "node scripts/inc-build.js && node scripts/generate-manifest.js && vite build",
    "build:local": "env-cmd -f .env.local yarn build && cd build && cross-var cpy . \"../builds/local/build-local-$npm_package_version/\"",
    "build:dev6": "env-cmd -f .env.dev6 yarn build && cd build && cross-var cpy . \"../builds/dev6/build-dev6-$npm_package_version/\"",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "start": "vite",
    "start:app:local": "env-cmd -f .env.local yarn start",
    "start:app:dev6": "env-cmd -f .env.dev6 yarn start",
    "lint": "eslint ./src --ext .ts,.tsx",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ci": "vitest run --coverage --passWithNoTests"
  },
  "browserslist": {
    "production": [
      "defaults",
      ">0.5%",
      "not dead",
      "not IE 11"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions"
    ]
  },
  "resolutions": {
    "@typescript-eslint/utils": "^8.8.1",
    "@typescript-eslint/type-utils": "^8.8.1",
    "@typescript-eslint/visitor-keys": "^8.8.1",
    "@typescript-eslint/scope-manager": "^8.8.1",
    "@typescript-eslint/types": "^8.8.1",
    "@typescript-eslint/typescript-estree": "^8.8.1"
  }
}